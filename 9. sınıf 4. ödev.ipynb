def Collatz_max(n):
  n_init = n
  max_number = n
  while n>1:
    if n%2 == 0: # % operatörü bölümden kalan sayıyı verir. Kendin dene istersen
       n = n/2
    else:
      n= 3*n +1
    if (n > max_number):
      max_number = n

